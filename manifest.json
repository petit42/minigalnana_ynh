{
  "name": "MinigalNano",
  "id": "minigalnano",
  "description": {
    "en": "Open Source Gallery",
    "fr": "Galerie Open Source"
  },
  "license": "AGPL-3",
  "developer": {
    "name": "petit",
    "email": "petit@peccadilles.net",
    "url": "https://github.com/petit42/minigalnano_ynh"
  },
  "multi_instance": "false",
  "arguments": {
    "install" : [
      {
        "name": "domain",
        "ask": {
            "en": "Choose a domain for MinigalNano Gallery",
            "fr": "Choisissez un domaine pour votre galerie MinigalNano""
        },
        "example": "domain.org"
      },
      {
        "name": "path",
        "ask": {
            "en": "Choose a path for MinagalNano gallery",
            "fr": "Choisissez un chemin pour votre galerie MinigalNano"
        },
        "example": "/gallery",
        "default": "/gallery"
      }
		{
		  "name": "name",
		  "ask": {
				"en": "Choose the name for MinigalNano gallery",
				"fr": "Choisissez le nom pour votre galerie MinigalNano" 
		  },
			"example": "My Own gallery",
			"default": "My own gallery"
		}
    ]
  }
}
